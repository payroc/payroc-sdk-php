{
  "name": "payroc/payroc",
  "version": "0.0.1438",
  "description": "Payroc PHP Library",
  "keywords": [
    "payroc",
    "api",
    "sdk"
  ],
  "license": [],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.4",
    "sentry/sentry": "^4.19"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "friendsofphp/php-cs-fixer": "3.5.0",
    "phpstan/phpstan": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "Payroc\\": "src/"
    },
    "files": [
      "src/SentryIntegration.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Payroc\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "build": [
      "@php -l src",
      "@php -l tests"
    ],
    "test": "phpunit",
    "analyze": "phpstan analyze src tests --memory-limit=1G"
  }
}